{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react",
    "stage-2",
    "flow"
  ],
  "plugins": [
    "transform-regenerator",
    "transform-async-to-generator"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "modules": true
        }],
        "react",
        "stage-2"
      ],
      "plugins": [
        "transform-regenerator",
        "transform-async-to-generator"
      ]
    },
    "production": {
      "presets": [
        "react-optimize"
      ],
      "comments": false
    }
  }
}
